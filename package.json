{
  "name": "@template-pro/ts-library",
  "version": "0.0.1",
  "preview": true,
  "description": "The TypeScript library template",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "scripts": {
    "start": "nodemon",
    "lint": "eslint src/*.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rimraf dist types",
    "prebuild": "npm run clean && tsc --emitDeclarationOnly",
    "build": "rollup -c",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "release-it --ci",
    "release:beta": "release-it --preReleaseId=beta --preRelease --ci",
    "release:minor": "release-it minor --ci",
    "release:minor:beta": "release-it minor --preReleaseId=beta --preRelease --ci"
  },
  "keywords": [
    "template",
    "typescript",
    "jest"
  ],
  "author": {
    "name": "Wxh16144",
    "email": "wxh16144@qq.com",
    "url": "https://github.com/wxh16144"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.27.0",
    "esno": "*",
    "jest": "^27.0.1",
    "jsdom": "16.6.0",
    "jsdom-global": "3.0.2",
    "nodemon": "^2.0.7",
    "release-it": "^14.7.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-esbuild": "^4.2.3",
    "ts-jest": "^27.0.0",
    "typescript": "^4.2.4"
  }
}
